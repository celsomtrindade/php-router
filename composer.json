{
    "name": "celsomtrindade/router",
    "description": "A lightweight and simple object oriented PHP Router - Forked from Bramus Router",
    "keywords": ["router", "routing"],
    "homepage": "https://github.com/celsomtrindade/php-router",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Celso M Trindade",
            "email": "celso@agdevision.com.br",
            "homepage": "https://agdevision.com.br"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "php-di/php-di": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8",
        "phpunit/php-code-coverage": "~2.0",
        "friendsofphp/php-cs-fixer": "~2.14"
    },
    "autoload": {
        "psr-0": {"Bramus": "src/"}
    },
    "scripts": {
        "test": "./vendor/bin/phpunit --colors=always",
        "lint": "php-cs-fixer fix --diff --dry-run",
        "fix": "php-cs-fixer fix"
    }
}
